{
  "name": "tsgrep",
  "displayName": "AST based search for JS/TS files",
  "description": "A VS Code extension with search, file preview, and navigation",
  "version": "1.0.0",
  "publisher": "shantanu2307",
  "author": "shantanu2307 <shantanugoyal23@gmail.com>",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:tsgrep.search"
  ],
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/shantanu2307/tsgrep-vscode-extension"
  },
  "contributes": {
    "commands": [
      {
        "command": "tsgrep.search",
        "title": "AST Search for JS/TS",
        "category": "Search"
      }
    ],
    "keybindings": [
      {
        "command": "tsgrep.search",
        "key": "ctrl+shift+f",
        "mac": "cmd+shift+f"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "rm -rf dist && webpack --mode production",
    "compile": "webpack --mode development",
    "watch": "webpack --watch --mode development",
    "test-compile": "tsc -p ./",
    "package": "vsce package --no-dependencies",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.5.2",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "tsgrep": "^4.3.0"
  }
}
